OBJECT       = FIELD
 NAME        = "ENERGY"
 DATA_TYPE   = ASCII_REAL
 UNIT        = "KEV"
 BYTES       = 8
 FORMAT      = "F8.5"
 DESCRIPTION = "Energy in keV converted from digital number (DN)"
END_OBJECT   = FIELD

OBJECT       = FIELD
 NAME        = "INTENSITY"
 DATA_TYPE   = ASCII_REAL
 UNIT        = "COUNT"
 BYTES       = 8
 FORMAT      = "F8.0"
 DESCRIPTION = "The log-scale intensity for each value of keV in column 1"
END_OBJECT   = FIELD
